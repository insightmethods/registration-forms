{
  "title": "2022-23 Family Registration Form",
  "subTitle": "",
  "type": "object",
  "required": ["dob", "gender", "ethnicity", "legalGuardian", "studentLiving"],
  "properties": {
    "fullName": {
      "type": "object",
      "title": "",
      "required": ["firstName", "lastName"],
      "properties": {
        "firstName": {
          "type": "string",
          "title": "First name"
        },
        "lastName": {
          "type": "string",
          "title": "Last name"
        }
      }
    },
    "dob": {
      "type": "string",
      "title": "Date of Birth",
      "format": "date"
    },
    "gender": {
      "type": "string",
      "title": "Gender",
      "enum": ["Male", "Female", "Others"]
    },
    "ethnicity": {
      "type": "string",
      "enum": ["Male", "Female", "Others"],
      "title": "Ethnicity",
      "default": ""
    },
    "primaryInfo": {
      "type": "object",
      "title": "Primary Household Information",
      "required": ["streetAddress"],
      "properties": {
        "streetAddress": {
          "type": "string",
          "title": "Street Address"
        },
        "city": {
          "type": "string",
          "title": "City"
        },
        "state": {
          "type": "string",
          "title": "State"
        },
        "postal": {
          "type": "string",
          "title": "Postal"
        }
      }
    },
    "legalGuardian": {
      "type": "string",
      "title": "Who is the legal guardian?"
    },
    "studentLiving": {
      "type": "object",
      "title": "With whom is the student living?",
      "properties": {
        "isStudentLivingParentsTogether": {
          "type": "boolean",
          "title": "Parents together"
        },
        "isStudentLivingParentsDivorced": {
          "type": "boolean",
          "title": "Parents divorced"
        },
        "isStudentLivingSingleParent": {
          "type": "boolean",
          "title": "Single parent"
        },
        "isStudentLivingMotherRemarried": {
          "type": "boolean",
          "title": "Mother remarried"
        },
        "isStudentLivingMotherDeceased": {
          "type": "boolean",
          "title": "Mother deceased"
        },
        "isStudentLivingFatherDeceased": {
          "type": "boolean",
          "title": "Father deceased"
        }
      }
    },
    "firstParent": {
      "type": "object",
      "title": "First Parent/Guardian",
      "required": ["primaryEmail"],
      "properties": {
        "fullName": {
          "type": "object",
          "title": "",
          "required": ["firstName", "lastName"],
          "properties": {
            "firstName": {
              "type": "string",
              "title": "First name"
            },
            "lastName": {
              "type": "string",
              "title": "Last name"
            }
          }
        },
        "primaryPhone": {
          "type": "object",
          "title": "Primary Phone/Cell Phone",
          "properties": {
            "isUseForAlert": {
              "type": "boolean",
              "title": "Use for Alert Phone Calls"
            },
            "primaryPhoneInput": {
              "type": "string",
              "title": " ",
              "maxLength": 10
            }
          }
        },
        "secondaryPhone": {
          "type": "object",
          "title": "Secondary Phone/Home Phone",
          "properties": {
            "isSecondaryPhoneForAlert": {
              "type": "boolean",
              "title": "Use for Alert Phone Calls"
            },
            "isSecondaryPhoneForText": {
              "type": "boolean",
              "title": "Use for Alert Text Messages"
            },
            "secondaryPhoneInput": {
              "type": "string",
              "title": " ",
              "pattern": "^[0-9]{10}$"
            }
          }
        },
        "primaryEmail": {
          "type": "string",
          "title": "Primary Email"
        },
        "relationshiptoStudent": {
          "type": "string",
          "title": "Relationship to Student"
        }
      }
    },
    "employmentInfo1": {
      "type": "object",
      "title": "Employment Information",
      "properties": {
        "employmentStatus": {
          "type": "string",
          "title": "Employment Status"
        },
        "occupation": {
          "type": "string",
          "title": "Title/Occupation"
        },
        "businessName": {
          "type": "string",
          "title": "Business Name"
        },
        "businessPhone": {
          "type": "string",
          "title": "Business Phone"
        },
        "businessAddress": {
          "type": "object",
          "title": "Business Address",
          "properties": {
            "streetAddress": {
              "type": "string",
              "title": "Street Address"
            },
            "city": {
              "type": "string",
              "title": "City"
            },
            "state": {
              "type": "string",
              "title": "State"
            },
            "postal": {
              "type": "string",
              "title": "Postal"
            }
          }
        }
      }
    },
    "secondParent": {
      "type": "object",
      "title": "Second Parent/Guardian",
      "properties": {
        "isSecondParent": {
          "type": "boolean",
          "title": "Is there a second parent/legal guardian in this household?"
        },
        "fullName": {
          "type": "object",
          "title": "",
          "required": ["firstName", "lastName"],
          "properties": {
            "firstName": {
              "type": "string",
              "title": "First name"
            },
            "lastName": {
              "type": "string",
              "title": "Last name"
            }
          }
        },
        "primaryPhone": {
          "type": "object",
          "title": "Primary Phone/Cell Phone",
          "properties": {
            "isUseForAlert": {
              "type": "boolean",
              "title": "Use for Alert Phone Calls"
            },
            "primaryPhoneInput": {
              "type": "string",
              "title": " ",
              "maxLength": 10
            }
          }
        },
        "secondaryPhone": {
          "type": "object",
          "title": "Secondary Phone/Home Phone",
          "properties": {
            "isSecondaryPhoneForAlert": {
              "type": "boolean",
              "title": "Use for Alert Phone Calls"
            },
            "isSecondaryPhoneForText": {
              "type": "boolean",
              "title": "Use for Alert Text Messages"
            },
            "secondaryPhoneInput": {
              "type": "string",
              "title": " ",
              "pattern": "^[0-9]{10}$"
            }
          }
        },
        "primaryEmail": {
          "type": "string",
          "title": "Primary Email"
        },
        "relationshiptoStudent": {
          "type": "string",
          "title": "Relationship to Student"
        }
      }
    },
    "employmentInfo2": {
      "type": "object",
      "title": "Employment Information",
      "properties": {
        "employmentStatus": {
          "type": "string",
          "title": "Employment Status"
        },
        "occupation": {
          "type": "string",
          "title": "Title/Occupation"
        },
        "businessName": {
          "type": "string",
          "title": "Business Name"
        },
        "businessPhone": {
          "type": "string",
          "title": "Business Phone"
        },
        "businessAddress": {
          "type": "object",
          "title": "Business Address",
          "properties": {
            "streetAddress": {
              "type": "string",
              "title": "Street Address"
            },
            "city": {
              "type": "string",
              "title": "City"
            },
            "state": {
              "type": "string",
              "title": "State"
            },
            "postal": {
              "type": "string",
              "title": "Postal"
            }
          }
        }
      }
    },
    "siblingInfo": {
      "title": "Sibling Information",
      "type": "object",
      "properties": {
        "maxSibling": {
          "type": "string",
          "title": "How many siblings, if any, does the applicant have?",
          "enum": ["1", "2", "3"]
        },
        "sibling1": {
          "title": "Sibling 1",
          "type": "object",
          "properties": {
            "fullName": {
              "type": "object",
              "title": "",
              "required": ["firstName", "lastName"],
              "properties": {
                "firstName": {
                  "type": "string",
                  "title": "First name"
                },
                "lastName": {
                  "type": "string",
                  "title": "Last name"
                }
              }
            },
            "relationshiptoStudent": {
              "type": "string",
              "title": "Relationship to Student"
            },
            "dob": {
              "type": "string",
              "title": "Date of Birth",
              "format": "date",
              "default": "DD/MM/YYYY"
            },
            "currentGrade": {
              "type": "string",
              "title": "Current Grade"
            },
            "currentSchool": {
              "type": "string",
              "title": "Current School"
            }
          }
        }
      }
    },
    "alternateInfo": {
      "type": "null",
      "title": "Alternate Emergency Contact(s) and Pick-up Authorization",
      "description": "If the parent(s)/guardian(s) identified above cannot be reached in an emergency, the following people are emergency contacts and are authorized to pick up the student from school and/or Extended Day Program. Please provide any additional adults authorized to pick up the student below. Only adults listed below will be allowed to pick up the student:"
    },
    "alertnateInfoDetails": {
      "type": "object",
      "title": "Alternate Emergency Contact 1",
      "properties": {
        "name": {
          "type": "string",
          "title": "Name"
        },
        "relationshiptoStudent": {
          "type": "string",
          "title": "Relationship to Student"
        },
        "phone": {
